msgid "User interface"
msgstr "Interface utilisateur"
msgid "Content types"
msgstr "Types de contenu"
msgid "General settings"
msgstr "Paramètres généraux"
msgid "Vocabularies"
msgstr "Vocabulaires"
msgid "Taxonomy terms"
msgstr "Termes de taxonomie"
msgid "Nodes"
msgstr "Nœuds"
msgid "Menus"
msgstr "Menus"
msgid "Menu items"
msgstr "Eléments de menu"
