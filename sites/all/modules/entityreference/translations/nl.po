msgstr "Node"
msgid "Display links"
msgstr "Links tonen"
msgid "View arguments"
msgstr "Bekijk argumenten"
msgid "Fields"
msgstr "Velden"
msgid "Contains"
msgstr "Bevat"
msgid "Mode"
msgstr "Modus"
msgid "Ascending"
msgstr "Oplopend"
msgid "Descending"
msgstr "Aflopend"
